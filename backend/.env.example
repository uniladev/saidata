# ============================================
# JWT Authentication Configuration
# ============================================
# Generate JWT secret with: php artisan jwt:secret
JWT_SECRET=
# Access token TTL in minutes (15 minutes recommended)
JWT_TTL=15
# Refresh token TTL in minutes (7 days = 10080 minutes)
JWT_REFRESH_TTL=10080
# JWT Algorithm
JWT_ALGO=HS256
# Enable token blacklist for logout
JWT_BLACKLIST_ENABLED=true
# Blacklist grace period in seconds
JWT_BLACKLIST_GRACE_PERIOD=0

# ============================================
# Campus SSO Configuration
# ============================================
# SSO Base URL (replace with your actual SSO endpoint)
SSO_BASE_URL=https://sso.unila.ac.id
# SSO API Key (get this from your SSO admin)
SSO_API_KEY=your_sso_api_key_here
# SSO request timeout in seconds
SSO_TIMEOUT=10
# Enable mock SSO for local development (set to false in production)
SSO_MOCK_ENABLED=true