# ========================================================
# VITE ENVIRONMENT VARIABLES
# ========================================================

# Backend API base URL
VITE_API_URL="http://127.0.0.1:8001/api/v1"

# Optional: If your backend uses a different auth prefix or port
# Example: VITE_API_URL="https://api.saidata.unila.ac.id"

# Environment mode
VITE_ENV="development"

# Public domain or app URL (for redirects, cookies, etc.)
VITE_APP_URL="http://localhost:5173"

# ========================================================
# APP CONFIGS (optional)
# ========================================================

# Default timeout for Axios (ms)
VITE_API_TIMEOUT="10000"

# Should Axios send credentials (cookies)?
VITE_API_WITH_CREDENTIALS="true"

# App name (for title, branding, etc.)
VITE_APP_NAME="Saidata Unila"

# Feature flags (optional)
VITE_FEATURE_ENABLE_SSO="false"
VITE_FEATURE_ENABLE_DEBUG="true"
